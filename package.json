{
    "name": "new-cli",
    "version": "1.0.0",
    "license": "MIT",
    "description": "",
    "repository": "",
    "contributors": [],
    "main": "./main.electron.js",
    "scripts": {
        "clean": "rimraf node_modules ngfactory doc build && npm cache clean",
        "clean:install": "npm run clean && npm install",
        "clean:build": "rimraf build",
        "clean:tmp": "rimraf tmp",
        "clean:ngfactory": "rimraf ngfactory && mkdir ngfactory",
        "bundle:closure": "java -jar node_modules/google-closure-compiler/compiler.jar --warning_level=QUIET --flagfile closure.conf --js_output_file ./build/bundle.js --output_manifest=closure/manifest.MF",
        "transpile:prod": "java -jar node_modules/google-closure-compiler/compiler.jar --warning_level=QUIET --language_in=ES6 --language_out=ES5 --js ./build/bundle.es2015.js --js_output_file ./build/bundle.js",
        "webdriver:update": "webdriver-manager update",
        "webdriver:start": "webdriver-manager start",
        "lint": "tslint --force \"src/app/**/*.ts\"",
        "e2e": "protractor protractor.config.js",
        "e2e:live": "protractor protractor.config.js --elementExplorer",
        "pretest": "",
        "test": "karma start karma.conf.js",
        "test:watch": "karma start karma.conf.js --no-single-run --auto-watch",
        "ci": "npm run e2e && npm run test",
        "ci:watch": "npm run e2e && npm run test:watch",
        "start": "ngr --build dev --watch --serve",
        "serve": "node server.js",
        "electron": "electron ./main.electron.js",
        "universal": "node server.universal.js",
        "postinstall": ""
    },
    "dependencies": {
        "@angular/animations": "6.0.0-rc.1",
        "@angular/common": "6.0.0-rc.1",
        "@angular/core": "6.0.0-rc.1",
        "@angular/forms": "6.0.0-rc.1",
        "@angular/http": "6.0.0-rc.1",
        "@angular/platform-browser": "6.0.0-rc.1",
        "@angular/platform-browser-dynamic": "6.0.0-rc.1",
        "@angular/platform-server": "6.0.0-rc.1",
        "@angular/router": "6.0.0-rc.1",
        "compression": "^1.7.0",
        "core-js": "^2.5.1",
        "express": "^4.15.0",
        "http": "^0.0.0",
        "https": "^1.0.0",
        "reflect-metadata": "^0.1.3",
        "rxjs": "6.0.0-beta.4",
        "rxjs-compat": "6.0.0-beta.4",
        "systemjs": "0.20.14",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@angular/compiler": "6.0.0-rc.1",
        "@angular/compiler-cli": "6.0.0-rc.1",
        "@angular/language-service": "6.0.0-rc.1",
        "@types/jasmine": "2.5.53",
        "@types/jasminewd2": "^2.0.3",
        "@types/karma": "1.7.1",
        "@types/node": "8.0.2",
        "autoprefixer": "^7.1.1",
        "babel-cli": "^6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "canonical-path": "0.0.2",
        "codelyzer": "^4.0.1",
        "css-mqpacker": "^6.0.1",
        "google-closure-compiler": "20180319.0.0",
        "jasmine-core": "^2.8.0",
        "jasmine-spec-reporter": "^4.2.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-htmlfile-reporter": "^0.3.5",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-mocha-reporter": "^2.2.3",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-remap-coverage": "^0.1.4",
        "livereload": "^0.6.2",
        "lodash": "^4.0.0",
        "node-sass": "^4.5.3",
        "opn": "^5.1.0",
        "phantomjs-prebuilt": "^2.1.14",
        "postcss": "^6.0.3",
        "postcss-cli": "^4.1.0",
        "postcss-csso": "^3.0.0",
        "protractor": "^5.1.2",
        "rimraf": "^2.6.2",
        "rollup": "^0.57.0",
        "shelljs": "^0.7.0",
        "ts-helpers": "^1.1.2",
        "ts-node": "^3.3.0",
        "tslint": "^5.4.3",
        "tslib": "^1.7.1",
        "typescript": "2.7.2"
    },
    "engines": {
        "node": ">8.0.0 <9.0.0"
    }
